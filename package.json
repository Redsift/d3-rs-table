{
  "name": "@redsift/d3-rs-table",
  "version": "0.1.0",
  "description": "Displays a 2D array as a HTML table using D3v4.",
  "keywords": [
    "table",
    "redsift",
    "d3",
    "visualization"
  ],
  "homepage": "https://github.com/redsift/d3-rs-table",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "module": "dist/d3-rs-table.esm.js",
  "jsnext:main": "dist/d3-rs-table.esm.js",
  "main": "dist/d3-rs-table.umd.min.js",
  "browser": "dist/d3-rs-table.umd.min.js",
  "esnext": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-table.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-table -g d3-selection serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated || true && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src"
  },
  "dependencies": {
    "d3-selection": "~1.0.2"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^0.3.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",    
    "@redsift/tape-reel": "~0.1.0",
    "browser-sync": "^2.14.0",
    "del": "^2.2.2",
    "eslint": "^3.3.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "rollup": "^0.34.10",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-commonjs": "^3.3.0",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-json": "^2.0.0",
    "rollup-stream": "^1.11.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^5.0.0"
  }
}
